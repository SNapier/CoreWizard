##########################################
# COREWIZARD-TEMPLATES V:{{ version }}           #
# AUTOMATICALLY GENERATED DO NOT EDIT    #
##########################################

#HOST DEFINITION
define  host{
    use             corewizard-generic-host
    host_name       {{ host.host_name }}
    address         {{ host.address }}
    check_command   {{ host.check_command }}
    hostgroups      {{ host.hostgroups }}
    action_url      {{ host.action_url }}
}

#BEGIN NCPA SERVICE/S DEFINITION
{% for service in services %}    
    define service {
        host_name           {{ host.host_name }}
        use                 corewizard-generic-service
        description         {{ services[service].service_description }}
        check_command       {{ services[service].command }}
        action_url          {{ services[service].action_url }}
    }

{% endfor %}
#END NCPA SERVICE/S DEFINITIONS
